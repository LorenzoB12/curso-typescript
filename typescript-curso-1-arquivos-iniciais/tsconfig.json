{
    "compilerOptions": {
        "outDir": "dist/js",
        "target": "ES6",
        "noEmitOnError": true,
        "noImplicitAny": true,
        "removeComments": true,
        "strictNullChecks": true,
        "experimentalDecorators": true,

        "strict": false,
        "useDefineForClassFields": false,
        "sourceMap": true
    },
    "include": ["app/**/*"]
}